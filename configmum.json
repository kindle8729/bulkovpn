{
	"agent": {
		"run_as_user": "root"
	},
	"logs": {
		"logs_collected": {
			"files": {
				"collect_list": [
					{
						"file_path": "/var/log/syslog",
						"log_group_name": "changeme",
						"log_stream_name": "{instance_id}"
					},
					{
						"file_path": "/var/log/mumble-server/mumble-server.log",
						"log_group_name": "mumbchange",
						"log_stream_name": "{instance_id}"
					}
				]
			}
		}
	}
}
